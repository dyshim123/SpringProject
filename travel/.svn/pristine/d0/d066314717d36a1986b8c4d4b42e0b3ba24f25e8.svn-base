   display: block;
   padding-bottom: 2em;
   padding-top: 1em;
}

#nlogo a {
   text-decoration: none;
   color: #000;
}

#nlogo span {
   padding-right: 0.5em;
   text-transform: uppercase;
   font-size: 0.90em;
   color: rgba(0, 0, 0, 0.3);
}

